{
  "name": "email",
  "version": "1.0.0",
  "description": "Transactional Email",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "clean": "rimraf .turbo node_modules dist",
    "coverage": "TS_NODE_FILES=true nyc mocha -r ts-node/register -r tsconfig-paths/register --timeout 999999 --exit --recursive \"src/__tests__/**/*.tests.ts\"",
    "build": "tsup",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format:write": "prettier --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{js,json,jsx,ts,tsx}\"",
    "start:dev": "ts-node src/index.ts",
    "docs": "npx typedoc",
    "count": "npx cloc .  -exclude-ext=csv --by-file-by-lang --exclude-dir=scripts,node_modules,build,.nyc_output,docs"
  },
  "author": "James Graham",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts}": [
      "gts fix"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.7",
    "@types/nodemailer": "^6.4.7",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "chai": "^4.3.6",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "lint-staged": "^13.0.3",
    "rewire": "^6.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.7.1",
    "sinon": "^14.0.1",
    "tsup": "^5.11.13",
    "typescript": "4.9.5",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "nodemailer": "^6.9.1"
  }
}
