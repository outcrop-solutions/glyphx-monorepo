{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2021"],
    "module": "NodeNext",
    "target": "ES2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
