{
  "name": "@glyphx/business",
  "version": "1.0.0",
  "description": "The core business logic controllers called from the Next.js lambda api interface",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "coverage": "TS_NODE_FILES=true nyc mocha -r ts-node/register -r tsconfig-paths/register --timeout 999999 --exit --recursive \"src/__tests__/**/*.tests.ts\"",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register -r tsconfig-paths/register --recursive  --timeout 999999 --exit \"src/__tests__/**/*.tests.ts\"",
    "integrationTest": "DOTENV_CONFIG_PATH=src/__integrationTests__/.env.integration TS_NODE_FILES=true mocha -r ts-node/register -r tsconfig-paths/register  -r dotenv/config --recursive  --timeout 999999 --exit \"src/__integrationTests__/**/*.tests.ts\"",
    "lint": "gts lint",
    "fix": "gts fix",
    "count": "npx cloc .  -exclude-ext=csv --by-file-by-lang --exclude-dir=scripts,node_modules,build,.nyc_output,docs,dist"
  },
  "author": "James M. Graham",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/types": "3.222.0",
    "@glyphx/eslint-config-glyphx-backend": "workspace:*",
    "@glyphx/tsconfig": "workspace:*",
    "@glyphx/types": "workspace:*",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.7",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.42.0",
    "aws-sdk-client-mock": "^2.0.1",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "rewire": "^6.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^5.11.13",
    "tsx": "^3.7.1",
    "typescript": "^4.8.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "3.223.0",
    "@aws-sdk/client-s3": "3.223.0",
    "@aws-sdk/client-secrets-manager": "3.223.0",
    "@aws-sdk/lib-storage": "3.223.0",
    "@glyphx/core": "workspace:*",
    "@glyphx/database": "workspace:*",
    "@glyphx/email": "workspace:*",
    "@stripe/stripe-js": "^1.23.0",
    "csv": "^6.2.2",
    "currency-symbol-map": "^5.1.0",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "express-validator": "^6.14.0",
    "logsnag": "^0.1.6",
    "mongoose": "^6.8.1",
    "next-auth": "4.15.0",
    "nodemailer": "^6.7.2",
    "slugify": "^1.6.5",
    "stripe": "^8.203.0",
    "validator": "^13.7.0"
  }
}
