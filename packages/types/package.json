  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "@types/node": "^18.11.7",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mongoose": "^6.8.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.13",
    "typedoc": "^0.23.20",
    "typedoc-plugin-not-exported": "^0.1.6",
    "typescript": "4.9.5",
    "@aws-sdk/client-s3": "^3.295.0"
  }
}
