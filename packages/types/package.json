  "main": "src/index.ts",
  "types": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SynGlyphX/serverless.git"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf .turbo node_modules dist",
    "fix": "eslint . --fix",
    "format:write": "prettier --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{js,json,jsx,ts,tsx}\"",
    "build": "tsup",
    "precommit": "npx lint-staged",
    "docs": "npx typedoc",
