{
  "src/fileProcessing/*.ts": {
    "type": "source",
    "alternate": "src/__tests__/fileProcessing/{}.tests.ts"
  },
  "src/__tests__/fileProcessing/*.tests.ts" : {
    "type" : "test",
    "alternate":  "src/fileProcessing/{}.ts"
  },
  "src/fieldProcessing/*.ts": {
    "type": "source",
    "alternate": "src/__tests__/fieldProcessing/{}.tests.ts"
  },
  "src/__tests__/fieldProcessing/*.tests.ts" : {
    "type" : "test",
    "alternate":  "src/fieldProcessing/{}.ts"
  },
  "src/aws/*.ts": {
    "type": "source",
    "alternate": "src/__tests__/aws/{}.tests.ts"
  },
  "src/__tests__/aws/*.tests.ts" : {
    "type" : "test",
    "alternate":  "src/aws/{}.ts"
  },
  "src/*.ts": {
    "type": "source",
    "alternate": "src/__tests__/{}.tests.ts"
  },
  "src/__tests__/*.tests.ts" : {
    "type" : "test",
    "alternate":  "src/{}.ts"
  }


}
