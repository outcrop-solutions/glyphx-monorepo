{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "fix": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "count": {
      "cache": false
    },
    "coverage": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "codegen": {
      "outputs": ["./packages/examples/**", "./packages/sdks/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "integrationTest": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "database#build": {
      "dependsOn": ["^build"],
      "env": ["DATABASE_URL"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "web#dev": {
      "dependsOn": ["^dev", "^build"],
      "cache": false
    }
  },
  "globalEnv": [
    "GITHUB_TOKEN",
    "API_ENV",
    "MONGODB_URI",
    "APP_URL",
    "DATABASE_URL",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "RECOIL_DUPLICATE_ATOM_KEY_CHECKING_ENABLED",
    "SENDGRID_API_KEY",
    "EMAIL_FROM",
    "EMAIL_SERVER",
    "SMTP_HOST",
    "SMTP_PORT",
    "SMTP_USERNAME",
    "SMTP_PASSWORD",
    "NODE_ENV",
    "AWS_KEY",
    "AWS_BUCKET",
    "SIGNER_URL",
    "AWS_SECRET",
    "AWS_REGION",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEXT_PUBLIC_PUBLISHABLE_KEY",
    "PAYMENTS_SIGNING_SECRET"
  ],
  "globalDependencies": ["tsconfig.json"]
}
