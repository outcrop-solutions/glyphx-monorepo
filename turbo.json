{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "fix": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "format:write": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "count": {
      "cache": false
    },
    "coverage": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "type-check": {
      "cache": false
    },
    "codegen": {
      "outputs": ["./packages/examples/**", "./packages/sdks/**"]
    },
    "build": {
      "outputs": [".next/**", "dist/**", "build/**", "**/*.node"]
    },
    "dev:build": {
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "demo:build": {
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "prod:build": {
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "dev:test": {
      "dependsOn": ["dev:build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "demo:test": {
      "dependsOn": ["demo:build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "prod:test": {
      "dependsOn": ["prod:build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "integrationTest": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "**/tests/**/*.ts", "**/tests/**/*.tsx"]
    },
    "dev:integrationTest": {
      "dependsOn": ["dev:build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "**/tests/**/*.ts", "**/tests/**/*.tsx"]
    },
    "demo:integrationTest": {
      "dependsOn": ["demo:build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "**/tests/**/*.ts", "**/tests/**/*.tsx"]
    },
    "prod:integrationTest": {
      "dependsOn": ["prod:build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "**/tests/**/*.ts", "**/tests/**/*.tsx"]
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": [
    "GLYPHX_ENV",
    "OPENAI_API_KEY",
    "ANTHROPIC_API_KEY",
    "HUGGINGFACE_API_KEY",
    "GITHUB_TOKEN",
    "API_ENV",
    "MONGODB_URI",
    "APP_URL",
    "DATABASE_URL",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "RECOIL_DUPLICATE_ATOM_KEY_CHECKING_ENABLED",
    "SENDGRID_API_KEY",
    "EMAIL_FROM",
    "EMAIL_SERVER",
    "NODE_ENV",
    "AWS_KEY",
    "AWS_BUCKET",
    "AWS_SECRET_ACCESS_KEY",
    "AWS_DEFAULT_REGION",
    "endpoint",
    "database",
    "user",
    "password",
    "databaseName",
    "AWS_ACCESS_KEY_ID",
    "SIGNER_URL",
    "AWS_SECRET",
    "AWS_REGION",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEXT_PUBLIC_PUBLISHABLE_KEY",
    "PAYMENTS_SECRET_KEY",
    "PAYMENTS_SIGNING_SECRET",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "LOG_LEVEL",
    "DATABASE_URL",
    "LIVEBLOCKS_SECRET_KEY",
    "NEXT_PUBLIC_GB_CLIENT_DEV",
    "NEXT_PUBLIC_GB_CLIENT_DEMO",
    "NEXT_PUBLIC_GB_CLIENT_PROD",
    "NEXT_PUBLIC_GB_KEY",
    "NEXT_PUBLIC_GB_ENV",
    "PROJECT_ID",
    "DEV_BLOB_READ_WRITE_TOKEN",
    "DEMO_BLOB_READ_WRITE_TOKEN",
    "PROD_BLOB_READ_WRITE_TOKEN",
    "DEV_BLOB_URL",
    "DEMO_BLOB_URL",
    "PROD_BLOB_URL",
    "BLOB_READ_WRITE_TOKEN",
    "DEV_CHAT_KV_REST_API_READ_ONLY_TOKEN",
    "DEV_CHAT_KV_REST_API_TOKEN",
    "DEV_CHAT_KV_REST_API_URL",
    "DEV_CHAT_KV_URL",
    "DEMO_CHAT_KV_REST_API_READ_ONLY_TOKEN",
    "DEMO_CHAT_KV_REST_API_TOKEN",
    "DEMO_CHAT_KV_REST_API_URL",
    "DEMO_CHAT_KV_URL",
    "KV_URL",
    "KV_REST_API_URL",
    "KV_REST_API_TOKEN",
    "KV_REST_API_READ_ONLY_TOKEN",
    "VERCEL_ENV"
  ],
  "globalDependencies": ["tsconfig.json"]
}
