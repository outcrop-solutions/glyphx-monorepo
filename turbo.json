{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "db:generate": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "dependsOn": ["db:generate"]
    },
    "db:push": {
      "cache": false,
      "env": ["MONGO_DEV", "MONGO_TEST", "MONGO_DEMO", "MONGO_PROD"]
    },
    "db:seed": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "count": {
      "cache": false
    },
    "coverage": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "codegen": {
      "outputs": ["./packages/examples/**", "./packages/sdks/**"]
    },
    "schema": {
      "dependsOn": ["db:generate", "db:push"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": [
        "SMTP_HOST",
        "SMTP_PORT",
        "SMTP_USERNAME",
        "SMTP_PASSWORD",
        "NEXTAUTH_URL",
        "EMAIL_SERVER",
        "EMAIL_FROM",
        "SENDGRID_API_KEY",
        "RECOIL_DUPLICATE_ATOM_KEY_CHECKING_ENABLED",
        "NEXTAUTH_SECRET"
      ],
      "outputs": [".next/**"]
    },
    "database#build": {
      "dependsOn": ["^build"],
      "env": ["DATABASE_URL"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "web#dev": {
      "dependsOn": ["^dev", "^build"],
      "cache": false,
      "env": [
        "SMTP_HOST",
        "SMTP_PORT",
        "SMTP_USERNAME",
        "SMTP_PASSWORD",
        "NEXTAUTH_URL",
        "EMAIL_SERVER",
        "EMAIL_FROM",
        "SENDGRID_API_KEY",
        "RECOIL_DUPLICATE_ATOM_KEY_CHECKING_ENABLED",
        "NEXTAUTH_SECRET"
      ]
    }
  },
  "globalEnv": ["GITHUB_TOKEN"],
  "globalDependencies": ["tsconfig.json"]
}
